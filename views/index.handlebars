<div class="container-fluid">
    <div class="row" id="navbar">
        <div class="col-10">
            <span class="navbar-brand">
                <img src="/media/logo.png" alt="" width="83px" height="72px">
            </span>
        </div>

        <div class="col-2 text-center">
            <a class="nav-link" href="#" data-toggle="modal" data-target="#log-sign"><span>Login
                    &nbsp;&nbsp;&nbsp;|&nbsp;&nbsp;&nbsp;
                    Signup</span></a>
        </div>
    </div>
</div>

<div id="container">
    <video autoplay muted loop>
        <source src="/media/coffee.mp4" type="video/mp4">
    </video>
    <div id="overlay"></div>
    <!-- Modal -->
    <div class="modal fade" id="log-sign" tabindex="-1" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div>
                    <ul class="nav nav-tabs">
                        <li id="tab1" class="active tab-style login-shadow"><a href="#signin" data-toggle="tab">Log
                                In</a></li>
                        <li id="tab2" class="tab-style"><a href="#signup" data-toggle="tab">Sign Up</a></li>
                    </ul>
                </div>
                <div class="modal-body">
                    <div class="tab-content">
                        <div class="tab-pane fade active in" id="signin">
                            <form class="form-horizontal">
                                <fieldset>
                                    <!-- Sign In Form -->
                                    <div class="group">
                                        <input required="" class="input" type="email" id="login-email"><span
                                            class="highlight"></span><span class="bar"></span>
                                        <label class="label" for="login-email">Email</label>
                                    </div>
                                    <div class="group">
                                        <input required="" class="input" type="password" id="login-password"><span
                                            class="highlight"></span><span class="bar"></span>
                                        <label class="label" for="login-password">Password</label>
                                    </div>
                                    <!-- Button -->
                                    <div class="control-group">
                                        <label class="control-label" for="signin"></label>
                                        <div class="controls">
                                            <button id="signin" name="signin" class="btn btn-primary btn-block">Log
                                                In</button>
                                        </div>
                                    </div>
                                </fieldset>
                            </form>
                        </div>
                        <div class="tab-pane fade" id="signup">
                            <form class="form-horizontal">
                                <fieldset>
                                    <!-- Sign Up Form -->
                                    <div class="group">
                                        <input required="" class="input" type="text" id="signup-firstname"><span
                                            class="highlight"></span><span class="bar"></span>
                                        <label class="label" for="signup-firstname">First Name</label>
                                    </div>
                                    <div class="group">
                                        <input required="" class="input" type="text" id="signup-lastname"><span
                                            class="highlight"></span><span class="bar"></span>
                                        <label class="label" for="signup-lastname">Last Name</label>
                                    </div>
                                    <div class="group">
                                        <input required="" class="input" type="email" id="signup-email"><span
                                            class="highlight"></span><span class="bar"></span>
                                        <label class="label" for="signup-email">Email</label>
                                    </div>
                                    <div class="group">
                                        <input required="" class="input" type="password" id="signup-password"><span
                                            class="highlight"></span><span class="bar"></span>
                                        <label class="label" for="signup-password">Password</label>
                                    </div>
                                    <!-- Button -->
                                    <div class="control-group">
                                        <label class="control-label" for="confirmsignup"></label>
                                        <div class="controls">
                                            <button id="confirmsignup" name="confirmsignup"
                                                class="btn btn-primary btn-block">Sign Up</button>
                                        </div>
                                    </div>
                                </fieldset>
                            </form>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

